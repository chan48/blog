{
  "version": "1.3.0",
  "name": "blog",
  "description": "rhostem's blog powered by Gatsby",
  "main": "n/a",
  "scripts": {
    "dev": "gatsby develop",
    "build": "npm run clean && gatsby build && node ampify.js && npm run copy:html",
    "serve:build": "npm run build && gatsby serve-build",
    "copy:html": "cp -r html/ public/",
    "clean:public": "rm -rf public/*",
    "clean": "npm run clean:public",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "jest --watch",
    "deploy": "npm run build && gh-pages -d public -r \"https://github.com/rhostem/rhostem.github.io.git\" -b \"master\""
  },
  "keywords": [
    "gatsby"
  ],
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "fbjs",
      "enzyme",
      "chai",
      "lodash",
      "redux",
      "react-redux",
      "react-router"
    ],
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/test/test.setup.js"
    ],
    "roots": [
      "<rootDir>/utils",
      "<rootDir>/components"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/test/fileMock.js",
      "\\.(css|less)$": "<rootDir>/client/test/styleMock.js"
    }
  },
  "author": "rhostem",
  "license": "MIT",
  "dependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "chai-jsx": "^1.0.1",
    "cheerio": "^0.22.0",
    "classnames": "^2.2.5",
    "enzyme": "^2.8.0",
    "front-matter": "^2.0.8",
    "fs-extra-promise": "^0.4.1",
    "gatsby": "^0.12.0",
    "history": "^2.1.1",
    "image-size": "^0.5.1",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "jsdom": "^9.12.0",
    "lodash": "^4.14.1",
    "markdown-it": "^6.0.2",
    "mkdirp": "^0.5.1",
    "moment": "^2.14.1",
    "node-sass": "^4.5.1",
    "postcss-short": "^3.0.1",
    "postcss-utilities": "^0.6.1",
    "precss": "^1.4.0",
    "ramda": "^0.23.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-ga": "^2.1.2",
    "react-helmet": "^5.0.1",
    "react-router": "^2.6.1",
    "recursive-readdir": "^2.1.1",
    "rimraf": "^2.6.1",
    "safe-access": "^0.1.0",
    "sitemap": "^1.8.2",
    "sync-request": "^4.0.1",
    "underscore.string": "^3.2.3",
    "url-loader": "^0.5.7"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.11.0",
    "image-webpack-loader": "^2.0.0",
    "lost": "^7.0.3",
    "null-loader": "^0.1.1",
    "postcss-loader": "^0.9.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.10",
    "rucksack-css": "^0.8.5",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1"
  }
}
